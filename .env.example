# .env.example

# Discord Bot Token (required)
DISCORD_TOKEN=your_discord_bot_token_here

# Spotify API Credentials (required for Spotify playback)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# Bot Configuration (optional)
PREFIX=!
DEFAULT_VOLUME=100
MAX_QUEUE_SIZE=100

# YouTube OAuth Refresh Token (optional - for better YouTube playback on cloud platforms)
# Get this from Lavalink logs after first OAuth authorization
YOUTUBE_REFRESH_TOKEN=

# YouTube Cookies (optional - alternative to OAuth, use Netscape format)
YOUTUBE_COOKIES=your_cookies_content
